package exam02;

import java.awt.BorderLayout;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class BoderLayoutTest extends JFrame {
	public BoderLayoutTest() {
		setTitle("BorderLayoutTest");
		setSize(300,150);
		
		setLayout();
		JButton button1 = new JButton("North");
		JButton button2 = new JButton("Center");
		JButton button3 = new JButton("West");
		JButton button4 = new JButton("East");
		JButton button5 = new JButton("South");
		
	}
	
	public static void main(String[] args) {
		BoderLayoutTest b = new BoderLayoutTest();

	}

}
