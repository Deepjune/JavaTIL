package exam01;

import javax.swing.*;
import java.awt.*;

public class MyFrame03 extends JFrame {
	public MyFrame03() {
		setSize(300, 150);
		setLocation(200, 300);
		setTitle("MyFrame");
		setLayout(new FlowLayout())
	}
	
	
	public static void main(String[] args) {
		MyFrame03 f = new MyFrame03();

	}

}
