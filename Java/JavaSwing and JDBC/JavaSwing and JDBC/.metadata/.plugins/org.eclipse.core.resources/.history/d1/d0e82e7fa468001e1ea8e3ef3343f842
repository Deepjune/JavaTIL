package exam03;

import javax.swing.JFrame;

public class Calculator extends JFrame {
	public Calculator() {
		setTitle("");
		setSize(300,300);
		
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
	
	public static void main(String[] args) {
		Calculator c = new Calculator();

	}

}
